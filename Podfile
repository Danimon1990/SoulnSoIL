# Uncomment the next line to define a global platform for your project
platform :ios, '18.0'
use_frameworks!

target 'Soul of SoIL' do
  # Amplify Core SDK
  use_frameworks!
  pod 'Amplify'

  # AWS Amplify Plugins
  pod 'AmplifyPlugins/AWSCognitoAuthPlugin'
  pod 'AmplifyPlugins/AWSAPIPlugin'
  pod 'AmplifyPlugins/AWSDataStorePlugin'


  # Optional for OAuth
  pod 'AppAuth'
end
